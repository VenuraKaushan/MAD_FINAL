<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:backgroundTint="@color/material_dynamic_tertiary20"
    tools:context=".HotelUI">

    <Button
        android:id="@+id/btnPlaceName1"
        android:layout_width="@dimen/hotel_name_width"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="64dp"
        android:backgroundTint="@color/butt"
        android:text="@string/hotel1"
        android:textColor="@color/black"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/btnPlaceName3"
        app:layout_constraintEnd_toStartOf="@+id/btnPlaceName4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.873" />

    <Button
        android:id="@+id/btnPlaceName2"
        android:layout_width="@dimen/hotel_name_width"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="100dp"
        android:backgroundTint="@color/butt"
        android:text="@string/hotel2"
        android:textColor="@color/black"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toTopOf="@+id/btnPlaceName4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.728"
        app:layout_constraintStart_toEndOf="@+id/btnPlaceName1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.942" />

    <Button
        android:id="@+id/btnPlaceName3"
        android:layout_width="@dimen/hotel_name_width"
        android:layout_height="wrap_content"
        android:layout_marginBottom="48dp"
        android:backgroundTint="@color/butt"
        android:text="@string/hotel3"
        android:textColor="@color/black"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnPlaceName4"
        app:layout_constraintHorizontal_bias="0.213"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnPlaceName4"
        android:layout_width="@dimen/hotel_name_width"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="48dp"
        android:backgroundTint="@color/butt"
        android:text="@string/hotel4"
        android:textColor="@color/black"
        app:cornerRadius="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="@dimen/mainImageView_width"
        android:layout_height="@dimen/mainImageView_height"
        android:layout_marginStart="24dp"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="465dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/hotel_banner" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="@dimen/hotel_Image_width"
        android:layout_height="@dimen/hotel_image_height"
        android:layout_marginStart="44dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="276dp"
        android:layout_marginBottom="274dp"
        android:contentDescription="TODO"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/galadari" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="@dimen/hotel_Image_width"
        android:layout_height="@dimen/hotel_image_height"
        android:layout_marginStart="264dp"
        android:layout_marginTop="330dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="274dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/grand_hotel" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="@dimen/hotel_Image_width"
        android:layout_height="@dimen/hotel_image_height"
        android:layout_marginStart="44dp"
        android:layout_marginTop="509dp"
        android:layout_marginEnd="276dp"
        android:layout_marginBottom="95dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ramada"
        tools:ignore="ImageContrastCheck" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="@dimen/hotel_Image_width"
        android:layout_height="@dimen/hotel_image_height"
        android:layout_marginStart="264dp"
        android:layout_marginTop="509dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="95dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/kingsburry" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="143dp"
        android:layout_marginTop="115dp"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="571dp"
        android:text="HOTELS"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="111dp"
        android:layout_marginTop="266dp"
        android:layout_marginEnd="111dp"
        android:layout_marginBottom="423dp"
        android:fontFamily="@font/aladin"
        android:text="@string/WhereToStayTxt"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>